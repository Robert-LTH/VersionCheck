<?xml version="1.0" encoding="UTF-8"?>
<application name="RStudio">
	<work>
		<versioncheck />
		<download />
	</work>
	<version>
		<unknown>
			<uri>https://www.rstudio.com/products/rstudio/release-notes/</uri>
			<patterns>
				<pattern>RStudio v(%5B0-9%5C.%5D%2B)</pattern>
			</patterns>
			<format></format>
		</unknown>
		<known>
			<pattern>^([0-9\.]+)</pattern>
			<format></format>
		</known>
	</version>
	<download>
		<uri>https://download1.rstudio.org/</uri>
    <patterns>
      <!-- All patterns should return a url which we can use for subsequent requests, the last pattern should return the url to the file which is downloaded -->
      <pattern>(RStudio-%5B0-9%5C.%5D%2B.exe)</pattern>
    </patterns>
  </download>
</application>